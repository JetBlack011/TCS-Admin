{
    "name": "TCS-Admin",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "This extension manages administrative activity on theCoderSchool's Chromebooks",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "background": {
        "scripts": [
            "js/background.js",
            "js/helpers.js",
            "js/wsc.js",
            "js/blocks.js",
            "js/cnc.js"
        ],
        "persistent": true
    },
    "permissions": [
        "storage",
        "tts",
        "history",
        "tabs",
        "http://localhost:8000/"
    ]
}